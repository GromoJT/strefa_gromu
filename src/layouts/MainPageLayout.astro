---
import MainHead from './MainHead.astro';
import Nav from '../components/Nav.astro';
import "../styles/global.css"
import Footer from '../components/Footer.astro';

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
    <MainHead title={title}/>
	<body>
        <Nav/>
		<slot />
        <Footer/>
	</body>
</html>

